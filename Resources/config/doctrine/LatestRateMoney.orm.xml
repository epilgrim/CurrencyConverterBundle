<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                    http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
	<entity name="Epilgrim\CurrencyConverterBundle\Entity\LatestRateMoney">
        <many-to-one field="currency" target-entity="Epilgrim\CurrencyConverterBundle\Entity\Currency">
            <join-column name="currency_id" referenced-column-name="id" />
        </many-to-one>
	</entity>
</doctrine-mapping>